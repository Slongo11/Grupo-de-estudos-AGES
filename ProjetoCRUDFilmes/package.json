{
    "name": "sprint3",
    "version": "1.0.0",
    "author": {
        "name": "Rodrigo Miotto Slongo"
    },
    "main": "app.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
        "lint": "eslint 'src/**/*.ts'",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
    },
    "prisma": {
        "schema": "./prisma/schema.prisma"
    },
    "devDependencies": {
        "@prisma/client": "^6.5.0",
        "@types/express": "^5.0.1",
        "@types/node": "^22.13.14",
        "dotenv": "^16.4.7",
        "eslint": "^9.23.0",
        "express": "^4.21.2",
        "install": "^0.13.0",
        "nodemon": "^3.1.9",
        "npm": "^11.2.0",
        "prettier": "^3.5.3",
        "prisma": "^6.5.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    },
    "dependencies": {
        "config": "^3.3.12"
    }
}
